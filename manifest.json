{
  "name": "New-Tab",
  "description": "Makes your new tab simple and more beautiful !",
  "version": "1.1.1",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "myPage.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./images/favicon-16x16.png",
      "32": "./images/favicon-32x32.png",
      "48": "./images/apple-touch-icon.png",
      "128": "./images/android-chrome-192x192.png"
    }
  },
  "icons": {
    "16": "./images/favicon-16x16.png",
    "32": "./images/favicon-32x32.png",
    "48": "./images/apple-touch-icon.png",
    "128": "./images/android-chrome-192x192.png"
  },
  "permissions": ["storage"]
}
